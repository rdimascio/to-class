{
  "name": "to-class",
  "version": "1.1.0",
  "description": "",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "all": "npm-run-all --sequential build lint cover",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "test": "ava --verbose",
    "cover": "nyc npm run test",
    "cover:report": "nyc report --reporter=text-lcov | coveralls",
    "build": "npm-run-all build:lib build:polyfill",
    "build:lib": "npx rimraf dist && npx babel --plugins '@babel/plugin-transform-modules-umd' src/index.js --out-file ./dist/index.js --no-comments --source-maps",
    "build:polyfill": "npx webpack --mode production"
  },
  "browserslist": [
    "> 1%",
    "ie 9"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": "xo --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --edit"
    }
  },
  "nyc": {
    "include": [
      "src/index.js"
    ],
    "all": true
  },
  "ava": {
    "require": [
      "esm"
    ],
    "nodeArguments": [
      "--experimental-modules"
    ]
  },
  "eslintConfig": {
    "extends": "eslint-config-xo"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rdimascio/to-class.git"
  },
  "keywords": [],
  "author": "Ryan DiMascio <ryan@dimasc.io> (https://ryan.dimasc.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rdimascio/to-class.git/issues"
  },
  "homepage": "https://github.com/rdimascio/to-class.git#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-modules-umd": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "ava": "^3.10.1",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "browser-env": "^3.3.0",
    "commitizen": "^4.1.2",
    "coveralls": "^3.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint-config-xo": "^0.32.0",
    "eslint-plugin-ava": "^10.4.0",
    "esm": "^3.2.25",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "semantic-release": "^17.1.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "xo": "^0.32.1"
  },
  "dependencies": {
    "core-js": "^3.6.5"
  }
}
